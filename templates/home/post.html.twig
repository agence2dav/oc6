{% extends 'base.html.twig' %}

{% block title %}{{ article.title | raw}}!{% endblock %}

{% block body %}

<div class="example-wrapper">

    <article>
        <h1>{{ article.title | raw}}</h1>
        <div class="metadatea">{{ article.date | date("d/m/Y") }}</div>
        <div class="content">
            <img src="{{ article.image | raw}}"/>
            {{ article.text | raw}}
        </div>
    </article>

</div>
{% endblock %}
